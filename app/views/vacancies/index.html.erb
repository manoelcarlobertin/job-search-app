<p style="color: green"><%= notice %></p>

<% content_for :title, "Vacancies" %>

<h1>Vacancies</h1>

<div id="vacancies">
  <% @vacancies.each do |vacancy| %>
    <%= render vacancy %>
    <p>
      <%= link_to "Show this vacancy", vacancy %>
    </p>
  <% end %>
</div>

<%= link_to "New vacancy", new_vacancy_path %>
